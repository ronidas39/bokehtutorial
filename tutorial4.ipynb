   "outputs": [],
   "source": [
    "from bokeh.plotting import figure ,show ,output_file"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "#set the plot dimension\n",
    "test_plot=figure(plot_height=400,plot_width=600)\n",
    "test_plot.line([2,4,6,8],[10,20,30,40])\n",
    "output_file(\"tutorial3.html\")\n",
    "show(test_plot)"
   ]
